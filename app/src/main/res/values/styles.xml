        <item name="actionOverflowButtonStyle">@style/Custom.Widget.ActionButton.Overflow</item>
    </style>

    <style name="Custom.Widget.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="maxButtonHeight">24dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="contentInsetStart">0dp</item>
    </style>

    <style name="MyCustomTabLayout" parent="Widget.Design.TabLayout">
        <item name="tabMaxWidth">@dimen/tab_max_width</item>
        <item name="tabIndicatorColor">?attr/colorAccent</item>
        <item name="tabIndicatorHeight">4dp</item>
        <item name="tabPaddingStart">2dp</item>
        <item name="tabPaddingEnd">2dp</item>
        <item name="tabGravity">fill</item>
        <item name="tabBackground">@color/tabBackgroundColor</item>
        <item name="tabTextAppearance">@style/MyCustomTabTextAppearance</item>
        <item name="tabSelectedTextColor">@color/colorPrimaryText</item>
    </style>
    <style name="MyCustomTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/tab_title_font_size</item>
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="Dialog.FullScreen" parent="Theme.AppCompat.Dialog">
        <item name="android:padding">0dp</item>
        <item name="android:windowBackground">@color/colorPrimary</item>
    </style>

    <style name="customDnDSwitch" parent="Theme.AppCompat.DayNight">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/colorSwitchCompactThumbGreen</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/dndSwitchOff</item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">@color/dndSwitchOff</item>
    </style>



    <style name="customEnableDisableSwitch" parent="Theme.AppCompat.DayNight">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/colorSwitchCompactThumbGreen</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/main_pink</item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">@color/warm_grey_two</item>
    </style>

